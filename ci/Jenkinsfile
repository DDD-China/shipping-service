node{
    step([$class: 'WsCleanup'])

    stage('Checkout') {
        git url: 'https://github.com/DDD-China/shipping-service.git', branch: 'master'
    }

    stage('Test') {
        sh './gradlew test'
    }

    stage('Build') {
        sh './gradlew build'
    }

    stage('Gen Image') {
        sh './ci/genImage.sh'
    }

    stage('Deploy') {
        sh './ci/deploy.sh'
    }
}